{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@context/*": ["context/*"],
      "@navigation/*": ["navigation/*"],
      "@themes/*": ["themes/*"],
      "@services/*": ["services/*"],
      "@hooks/*": ["hooks/*"],
      "@data/*": ["data/*"],
      "@i18n/*": ["i18n/*"],
      "@store/*": ["store/*"]
    }
  }
}
